{
  "name": "playmaster",
  "version": "0.0.0",
  "description": "evolition.io presents playmaster, your table top roleplaying character manager",
  "main": "app/",
  "engineStrict": true,
  "build": {
    "appId": "io.ev.playmaster",
    "copyright": "Copyright (c) 2017, Evolition, LLC. All rights reserved.",
    "productName": "Evolitiotn - Playmaster",
    "forceCodeSigning": false
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "clean": "rm -rf ./dist",
    "dist": "electron-packager ./ Playmaster --arch=x64 --out=dist",
    "mac-dist": "NODE_ENV=production build -m --x64",
    "win-dist": "NODE_ENV=production build -w --x64",
    "pack:osx": "npm run clean && electron-packager . \"Playmaster\" --arch=x64 --out=dist/osx --platform=darwin",
    "start": "NODE_ENV=development babel-node scripts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evolition/playmaster.git"
  },
